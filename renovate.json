{
  "extends": [
    "config:base"
  ],
  "rebaseWhen": "behind-base-branch",
  "enabledManagers": [
    "maven",
    "npm"
  ],
  "packageRules": [
    {
      "groupName": "CKEditor dev",
      "matchPackagePrefixes": [
        "@ckeditor/ckeditor5-dev"
      ]
    },
    {
      "groupName": "CKEditor",
      "matchPackagePrefixes": [
        "@ckeditor/ckeditor5-"
      ]
    }
  ]


}
